{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "max-len": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "implicit-arrow-linebreak": 0,
    "no-underscore-dangle": ["error", { "allow": ["_normalizeDomain"] }]
  }
}
